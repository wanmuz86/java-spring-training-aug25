package com.example.helloworld;

public class ForLoopDemo {

	public static void main(String[] args) {
		// From 0 (dari 0)
		// i < 10 = Until 10
		// i++ <- every iteration add by 1 i++
		System.out.println("Increment loop");
		for (int i = 0; i < 10; i++) {
			System.out.println("Number is "+i);
		}
	}

}
